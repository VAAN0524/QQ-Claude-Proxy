{
  "name": "qq-claude-proxy",
  "version": "1.0.0",
  "description": "QQ Remote Control for Claude Code - 通过QQ远程控制本地Claude Code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:win": "chcp 65001 >nul && tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "daily-cleanup": "tsx scripts/daily-memory-cleanup.ts",
    "monitor": "tsx src/cli/monitor.ts",
    "watchdog": "tsx scripts/watchdog.ts",
    "watchdog:start": "tsx scripts/watchdog.ts start",
    "watchdog:stop": "tsx scripts/watchdog.ts stop",
    "watchdog:status": "tsx scripts/watchdog.ts status",
    "install-startup": "node scripts/install-startup",
    "uninstall-startup": "node scripts/install-startup"
  },
  "keywords": [
    "qq",
    "bot",
    "claude",
    "ai",
    "remote-control"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.77.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@types/form-data": "^2.2.1",
    "axios": "^1.13.5",
    "cheerio": "^1.2.0",
    "dotenv": "^17.3.1",
    "ducksearch": "^1.0.3",
    "form-data": "^4.0.5",
    "https-proxy-agent": "^7.0.6",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "pptxgenjs": "^4.0.1",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
